<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="gray.light.owner.repository.ProjectDetailsRepository">

    <insert id="save"
            parameterType="ProjectDetails"
            keyProperty="originId,type">
        insert into project_details (origin_id, type, http, version, status)
        values (#{originId}, #{type}, #{http}, #{version}, #{status})
    </insert>

</mapper>